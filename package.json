{
  "name": "movies-web-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "node-sass": "^4.5.3",
    "nouislider": "^10.1.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-dynamic-modal": "^1.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-scripts": "1.0.10",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6"
  },
  "scripts": {
    "deploy": "yarn run build && aws s3 sync build/ s3://movies-web-app --exclude *.map --delete",
    "build-css": "node-sass src/styles/scss/ -o src/styles/css/",
    "watch-css": "npm run build-css && node-sass src/styles/scss/ -o src/styles/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
